pragma solidity >=0.7.0 <0.9.0;

///@title Storage
///@ dev Store and retrieve value in a variable

contract Storage{
    uint256 number;
    /// @dev store value
    /// @param num value

// function that sets a value in a variable
    function store(uint256 num) public {
        number=num;
    }
    ///@dev returv value
    ///@return value- number

    function retrieve() public view returns (uint256){
        return number; 
    }


}
